{
  "name": "@asteroid-mdx/cli",
  "version": "1.0.0",
  "types": "./module/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "import": "./module/index.js",
    "node": "./dist/index.js"
  },
  "bin": {
    "asteroid": "bin/asteroid.js"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "shx rm -rf dist module",
    "build": "run-p build:*",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly --outDir module",
    "predev": "shx rm -rf dist",
    "dev": "tsc -w --incremental -p tsconfig.dev.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/node12": "^1.0.7",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.40.0",
    "shx": "^0.3.3",
    "typescript": "^4.1.2",
    "vite": "^2.0.5"
  }
}
